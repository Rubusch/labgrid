lab:
  location: rpi
  USBSerialPort:    # serial /dev/ttyUSB0
    match:
      ID_SERIAL_SHORT: FTBW2Z1T
    speed: 115200

  NetworkInterface: # ethernet
    ifname: eth0

  USBSDMuxDevice:
    match:
      ID_PATH: platform-xhci-hcd.0-usb-0:1.1

  USBMassStorage:
    match:
      ID_PATH: platform-xhci-hcd.0-usb-0:1.1:1.0-scsi-0:0:0:0

#  SysfsGPIO:        # PIN26 [37] - a.k.a. GPIO425 to power the DUT
#    index: 425       # 425
